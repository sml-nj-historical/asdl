SHELL =			/bin/sh
PREFIX =		@prefix@

.PHONY:		pdf clean distclean devclean

pdf:		manual.pdf

manual.pdf:	$(wildcard *.tex) $(wildcard code/*.asdl)
	latexmk -pdf manual

.PHONY:		clean distclean devclean

clean:
	latexmk -C manual

distclean:	clean
	rm -f Makefile.in

devclean:	distclean
