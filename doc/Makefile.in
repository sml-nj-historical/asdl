SHELL =			/bin/sh
PREFIX =		@prefix@

DOC_DIRS =		manual

pdf:
	for dir in $(DIRS) ; do \
	    (cd $$dir; make pdf) \
	done

clean:
	for dir in $(DIRS) ; do \
	    (cd $$dir; make clean) \
	done
