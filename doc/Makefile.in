SHELL =		/bin/sh
PREFIX =	@prefix@

DIRS =		manual-3.0

.PHONY:		pdf local-install install

pdf:
	for dir in $(DIRS) ; do \
	    (cd $$dir; make pdf) \
	done

local-install:

install:

#################### Cleanup ####################

CLEAN_SUBDIRS =		$(DIRS)

DISTCLEAN_FILES +=      Makefile

DEVCLEAN_FILES =

include @ASDL_MKDIR@/clean-rules.gmk
